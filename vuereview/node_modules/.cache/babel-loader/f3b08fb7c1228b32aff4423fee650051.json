n        >\r\n        </el-option>\r\n      </el-select>\r\n      <el-select v-model=\"type\" style=\"margin-left: 5px\" placeholder=\"选择论文类型\">\r\n        <el-option\r\n            v-for=\"item in typeOptions\"\r\n            :key=\"item.type\"\r\n            :label=\"item.label\"\r\n            :value=\"item.type\"\r\n        >\r\n        </el-option>\r\n      </el-select>\r\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" border style=\"width: 100%\">\r\n      <el-table-column prop=\"id\" v-if=false label=\"序号\" />\r\n      <el-table-column prop=\"thesisid\" label=\"论文编号\" />\r\n      <el-table-column prop=\"title\" label=\"论文题目\" />\r\n      <el-table-column prop=\"department\" label=\"所属系\"  />\r\n      <el-table-column prop=\"comment\" label=\"评审意见\"  />\r\n      <el-table-column prop=\"voters\" label=\"投票人数\" />\r\n      <el-table-column prop=\"noes\" label=\"未通过票数\"  />\r\n      <el-table-column prop=\"yesses\" label=\"通过票数\"  />\r\n      <el-table-column prop=\"results\" label=\"建议结果\"  />\r\n      <el-table-column fixed=\"right\" label=\"确认结果\" width=\"160\">\r\n        <template #default=\"scope\">\r\n        <el-button type=\"text\" @click=\"centerDialogVisible = true\"\r\n        >选择</el-button\r\n        >\r\n        <el-dialog v-model=\"centerDialogVisible\" title=\"确认结果\" width=\"30%\" center>\r\n      <span>\r\n        <el-table-column prop=\"results\" label=\"建议结果\"  />\r\n  <!--      <div id='app'>-->\r\n  <!--      <input type=\"radio\" name=\"test\" v-for=\"(item,index) in list\" :value=\"item.value\" v-model=\"checkedValue\">-->\r\n  <!--      <button @click=\"getRadioVal\">获取选中的值</button>-->\r\n  <!--      </div>-->\r\n        <div>\r\n      <el-radio v-model=\"radio1\" label=\"5\">通过</el-radio>\r\n      <el-radio v-model=\"radio1\" label=\"6\">不通过</el-radio>\r\n        </div>\r\n\r\n        <!--      <div>-->\r\n  <!--    <el-radio v-model=\"radio1\" label=\"1\">通过</el-radio>-->\r\n  <!--    <el-radio v-model=\"radio1\" label=\"2\">不通过</el-radio>-->\r\n  <!--     </div>-->\r\n      </span>\r\n          <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"centerDialogVisible = false\">取消</el-button>\r\n\r\n            <el-button type=\"primary\" @click=\"getresults(scope.row)\">确认</el-button>\r\n\r\n\r\n        </span>\r\n          </template>\r\n        </el-dialog>\r\n  <!--      <template #default=\"scope\">-->\r\n  <!--        <el-button size=\"mini\"  @click=\"handleEdit(scope.row)\">确认</el-button>-->\r\n  <!--        &lt;!&ndash;        <el-popconfirm title=\"确定要删除吗?\" @confirm=\"handleDelete(scope.row.id)\">&ndash;&gt;-->\r\n  <!--        &lt;!&ndash;          <template #reference>&ndash;&gt;-->\r\n  <!--        &lt;!&ndash;            <el-button size=\"mini\" type=\"danger\" >删除</el-button>&ndash;&gt;-->\r\n  <!--        &lt;!&ndash;          </template>&ndash;&gt;-->\r\n  <!--        &lt;!&ndash;        </el-popconfirm>&ndash;&gt;-->\r\n  <!--      </template>-->\r\n\r\n        </template>\r\n      </el-table-column>\r\n  <!--    <div style=\"margin: 10px 0\">-->\r\n  <!--      <el-pagination-->\r\n  <!--          v-model:currentPage=\"currentPage\"-->\r\n  <!--          :page-sizes=\"[5, 10, 20]\"-->\r\n  <!--          :page-size=\"pageSize\"-->\r\n  <!--          layout=\"total, sizes, prev, pager, next, jumper\"-->\r\n  <!--          :total=\"total\"-->\r\n  <!--          @size-change=\"handleSizeChange\"-->\r\n  <!--          @current-change=\"handleCurrentChange\">-->\r\n  <!--      </el-pagination>-->\r\n  <!--    </div>-->\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport { ref, defineComponent } from 'vue'\r\nexport default {\r\n  name: \"CommitteeVote\",\r\n  data() {\r\n    return {\r\n      distinctYear:[],\r\n      academicYear:'',\r\n      semOptions:ref([\r\n        {\r\n          semester: '第一学期',\r\n          label: '第一学期',\r\n        },\r\n        {\r\n          semester: '第二学期',\r\n          label: '第二学期',\r\n        }]),\r\n      semester:ref(''),\r\n      typeOptions:ref([\r\n        {\r\n          type: '学硕',\r\n          label: '学硕',\r\n        },\r\n        {\r\n          type: '专硕',\r\n          label: '专硕',\r\n        }]),\r\n      type:ref(''),\r\n      batch:'',\r\n      checkedValue:'',\r\n      // list:[{value:1,name:\"通过\"},{value:2,name:\"通过\"}],\r\n      form:{},\r\n      // radio1: ref('1'),\r\n      centerDialogVisible: ref(false),\r\n      // search:'',\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      tableData:[],\r\n      radioVal: \"\",\r\n      radio1:\"\",\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  mounted() {\r\n    this.getYear();\r\n  },\r\n  methods: {\r\n    // watch:{\r\n    //   checkedValue:function(){\r\n    //     this.answer=[];\r\n    //     this.answer.push(this.checkedValue);\r\n    //   }\r\n    // },\r\n    getYear(){\r\n      request.put(\"/batch/distinctYear\").then(res=>{\r\n        console.log(res)\r\n        this.distinctYear=res.data\r\n        this.total=res.data.total\r\n      })\r\n    },\r\n    getBatch(){\r\n      request.get(\"/batch/getbatchID\",{\r\n        params:{\r\n          year:this.academicYear,\r\n          semester:this.semester,\r\n          type:this.type\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.batch=res.data\r\n        this.total=res.data.total\r\n      })\r\n    },\r\n    load() {\r\n      // let userJson = sessionStorage.getItem(\"user\");\r\n      // let user = JSON.parse(userJson)\r\n      // console.log(user.username)\r\n      this.getBatch()\r\n      request.get(\"/committeevote\",{\r\n        params: {\r\n          batch:this.batch\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.tableData=res.data\r\n        this.total=res.data.total\r\n      })\r\n\r\n    },\r\n    // test() {\r\n    //   console.log('被选中的值为:' + this.checkedValue)\r\n    // },\r\n    getRadioVal() {\r\n      console.log(this.radio1);\r\n    },\r\n    // getRadioVal () {\r\n    //   this.radioVal=JSON.parse(JSON.stringify(checkedValue));\r\n      // saveTeacher(thesisid) {\r\n      //   request.post(\"/committeevote\", this.form).then(res => {\r\n      //     console.log(res)\r\n      //     this.form.status = ratio1;\r\n      //   })\r\n      // },\r\n    // },\r\n    getresults(row){\r\n      console.log(this.radio1);\r\n      this.form = JSON.parse(JSON.stringify(row));\r\n      this.form.status=this.radio1\r\n      console.log(this.form.status)\r\n      console.log(this.form)\r\n      request.put(\"/thesis\", this.form).then(res => {\r\n        if (res.code==='0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"论文内审结果确认成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      });\r\n      this.load();\r\n      this.load();\r\n      this.centerDialogVisible = false;\r\n      this.load();\r\n      this.load();\r\n      // request.put(\"/thesis\", this.form).then(res2 => {\r\n      //   console.log(res2)\r\n      //   if (res2.code === '0') {\r\n      //     this.$message({\r\n      //       type: \"success\",\r\n      //       message: \"确认成功\"\r\n      //     })\r\n      //   } else {\r\n      //     this.$message({\r\n      //       type: \"error\",\r\n      //       message: res2.msg\r\n      //     })\r\n      //     // console.log(e)\r\n      //   }\r\n      // })\r\n    },\r\n\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.el-pagination {\r\n  text-align: center;\r\n}\r\n</style>"],"sourceRoot":""}]}