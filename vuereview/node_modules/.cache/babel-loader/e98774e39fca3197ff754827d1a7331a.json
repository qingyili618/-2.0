answer.push(this.checkedValue);\r\n    //   }\r\n    // },\r\n    getYear(){\r\n      request.put(\"/batch/distinctYear\").then(res=>{\r\n        console.log(res)\r\n        this.distinctYear=res.data\r\n        this.total=res.data.total\r\n      })\r\n    },\r\n    getBatch(){\r\n      request.get(\"/batch/getbatchID\",{\r\n        params:{\r\n          year:this.academicYear,\r\n          semester:this.semester,\r\n          type:this.type\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.batch=res.data\r\n        this.total=res.data.total\r\n      })\r\n    },\r\n    load() {\r\n      // let userJson = sessionStorage.getItem(\"user\");\r\n      // let user = JSON.parse(userJson)\r\n      // console.log(user.username)\r\n      this.getBatch()\r\n      request.get(\"/committeevote\",{\r\n        params: {\r\n          batch:this.batch\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.tableData=res.data\r\n        this.total=res.data.total\r\n      })\r\n\r\n    },\r\n    // test() {\r\n    //   console.log('被选中的值为:' + this.checkedValue)\r\n    // },\r\n    getRadioVal() {\r\n      console.log(this.radio1);\r\n    },\r\n    // getRadioVal () {\r\n    //   this.radioVal=JSON.parse(JSON.stringify(checkedValue));\r\n      // saveTeacher(thesisid) {\r\n      //   request.post(\"/committeevote\", this.form).then(res => {\r\n      //     console.log(res)\r\n      //     this.form.status = ratio1;\r\n      //   })\r\n      // },\r\n    // },\r\n    getresults(row){\r\n      console.log(this.radio1);\r\n      this.form = JSON.parse(JSON.stringify(row));\r\n      this.form.status=this.radio1\r\n      console.log(this.form.status)\r\n      console.log(this.form)\r\n      request.put(\"/thesis\", this.form).then(res => {\r\n        if (res.code==='0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"论文内审结果确认成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      });\r\n      this.load();\r\n      this.load();\r\n      this.centerDialogVisible = false;\r\n      this.load();\r\n      this.load();\r\n      // request.put(\"/thesis\", this.form).then(res2 => {\r\n      //   console.log(res2)\r\n      //   if (res2.code === '0') {\r\n      //     this.$message({\r\n      //       type: \"success\",\r\n      //       message: \"确认成功\"\r\n      //     })\r\n      //   } else {\r\n      //     this.$message({\r\n      //       type: \"error\",\r\n      //       message: res2.msg\r\n      //     })\r\n      //     // console.log(e)\r\n      //   }\r\n      // })\r\n    },\r\n\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}